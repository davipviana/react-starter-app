{
  "name": "react-starter-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dotenv": "^8.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "redux": "^4.0.4"
  },
  "scripts": {
    "start": "node scripts/custom-react-scripts.js start",
    "build": "node scripts/custom-react-scripts.js build",
    "test": "node scripts/custom-react-scripts.js test",
    "eject": "node scripts/custom-react-scripts.js eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/Config/ServiceWorker.js",
      "!src/Tests/**",
      "!**/node_modules/**",
      "!build/**"
    ]
  }
}
